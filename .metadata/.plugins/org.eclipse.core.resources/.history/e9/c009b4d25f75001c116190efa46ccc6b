package Calculadora;

public class CalculadoraTest {

	public static void main(String[] args) {
		
		Operaciones operaciones = new Operaciones();
		Calculadora suma = operaciones.suma(10.5, 5.2);
		Calculadora resta = operaciones.resta(10.5, 5.2);
		System.out.println(suma.getResult());
		System.out.println(resta.getResult());
		
		System.out.println("----------------------------------------------");
		
		Calculadora suma2 = operaciones.suma(10.5, 5.2);
		Calculadora multiplicacion = operaciones.multiplicacion(suma2.getResult(), 10.0);
		System.out.println(multiplicacion.getResult());
		
		
		
		

	}

}
