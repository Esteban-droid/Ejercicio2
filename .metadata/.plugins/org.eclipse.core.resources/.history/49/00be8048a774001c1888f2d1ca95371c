package Abstracto;

public class PhoneTester {

	public static void main(String[] args) {
		
		System.out.println("Phone Iphone");
		IPhone iphone = new IPhone("N48493S",90,"Disponible","default");
		iphone.displayInfo();
		iphone.ring();
		iphone.unlock();
		//
		System.out.println("--------------------------------------");
		System.out.println("Phone galaxy");
		
		Galaxy galaxy = new Galaxy("S20 2021", 10, "Disponible", "default");
		galaxy.ring();
		galaxy.unlock();
		galaxy.displayInfo();

	}

}
