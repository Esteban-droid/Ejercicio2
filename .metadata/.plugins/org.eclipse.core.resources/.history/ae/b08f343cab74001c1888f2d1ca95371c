package Static;

public class BankAccountTest {

	public static void main(String[] args) {

		BankAccount.depositar('C',1500);
		System.out.println("Saldo de cuenta corriente:" + BankAccount.getsCorriente());
		
		BankAccount.depositar('A',8543);
		System.out.println("Saldo cuenta de ahorro:" +BankAccount.getsAhorro());
		
		BankAccount.depositar('A',1000);
		System.out.println("Saldo cuenta de ahorro:" +BankAccount.getsAhorro());
		
		BankAccount.retirar('C', 100);
		System.out.println("Saldo de cuenta corriente:" + BankAccount.getsCorriente());
		
		System.out.println("----------------------------------------------------");
		System.out.println(BankAccount.verTotal());
		System.out.println("----------------------------------------------------");
	}

}
