public class FizzBuzz {

    public String fizzBuzz(int number) {
        // lógica de fizzbuzz aquí
		
String dato;

		if(number%3==0) {

		    System.out.println("Fizz");
		    dato = "Fizz";

		} else if(number%5==0) {

		    System.out.println("Buzz");

		} else {
		    System.out.println("El numero es divisible por 3 y 5");

		}
		
    }

    
}
