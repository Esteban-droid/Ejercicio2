public class FizzBuzz {

    public String fizzBuzz(int number) {
        // lógica de fizzbuzz aquí

		if(number%3==0) {
		    return "Fizz";

		} else if(number%5==0) {
		    return "Buzz";

		} else {
		    return "El numero es divisible por 3 y 5";

		}
		
		return String.valueOf(number);
		
    }
    
}
