class Person {

    private int age;

    private String name;

    public Person(int ageParam, String nameParam) {

        this.age = ageParam;

        this.name = nameParam;
        
    //-------this...Lo mismo que esto--------------
        
        private int age;

        private String name;

        public Person(int ageParam, String nameParam) {

            age = ageParam;

            name = nameParam;

        }   

    }
    
    

}